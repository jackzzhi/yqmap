{
  "name": "overduedata",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:test": "set NODE_ENV=test && vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:test": "set NODE_ENV=test && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint2": "eslint --fix --ext .js,.vue src test/unit"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dns": "^0.2.2",
    "echarts": "^4.2.0-rc.2",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "fetch": "^1.1.0",
    "hash.js": "^1.1.7",
    "jquery": "^1.12.4",
    "lib-flexible": "^0.3.2",
    "mint-ui": "^2.2.13",
    "vue": "^2.5.21",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-plugin-eslint": "^3.0.3",
    "@vue/cli-service": "^3.0.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "lodash": "^4.17.11",
    "postcss-loader": "^3.0.0",
    "postcss-px2rem": "^0.3.0",
    "qs": "^6.6.0",
    "sockjs-client": "^1.3.0",
    "stompjs": "^2.3.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-debugger": 1,
      "indent": [
        0,
        1
      ],
      "no-console": 0,
      "no-unused-vars": 1,
      "padded-blocks": 0,
      "space-before-blocks": 1,
      "no-unreachable": 1,
      "space-infix-ops": 1,
      "keyword-spacing": 1,
      "arrow-spacing": 1,
      "object-property-newline": 1,
      "no-irregular-whitespace": 0,
      "no-cond-assign": 2,
      "array-bracket-spacing": [
        2,
        "never"
      ],
      "comma-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "semi-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
